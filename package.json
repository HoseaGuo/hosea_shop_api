{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev-ts": "ts-node ./src/app.ts",
    "nodemon-dist": "nodemon --config nodemon-dist.json ./dist/app.js",
    "tsc": "tsc --watch",
    "serve": "chcp 65001 && concurrently \"npm run tsc\" \"npm run nodemon-dist\"",
    "dev": "nodemon --config nodemon-ts.json ./src/app.ts",
    "build": "rimraf ./dist/* && tsc",
    "deploy": "ts-node deploy.ts",
    "dis": "npm run build && npm run deploy",
    "webpack": "npx webpack build",
    "dis:www": "cross-env KEY=www \"npm run webpack && npm run deploy\"",
    "dis:test": "cross-env KEY=test \"npm run webpack && npm run deploy\""
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "axios": "^0.21.4",
    "csprng": "^0.1.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.3",
    "koa-body": "^4.2.0",
    "koa2-cors": "^2.0.6",
    "mongodb": "^4.1.2",
    "mongoose": "^6.0.7",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@swc/core": "^1.2.111",
    "@swc/helpers": "^0.2.14",
    "@types/csprng": "^0.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa2-cors": "^2.0.2",
    "@types/koa__router": "^8.0.8",
    "@types/node": "^16.9.6",
    "compressing": "^1.5.1",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "node-ssh": "^12.0.2",
    "nodemon": "^2.0.13",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "string-replace-loader": "^3.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
}
